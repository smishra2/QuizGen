{
	"manifest_version":2,
    
    "name": "QuizGen",
    "description": "One click quiz generation for web content.",
    "version": "1.0",
    
    "permissions": [
        "tabs",
        "http://127.0.0.1:5000/"
    ],
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "default.html"
    }
}
